<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="gov.anl.aps.irmis.persistence.pv.IOC" 
         table="ioc_table" mutable="false">
    
    <id name="id" column="ioc_id" type="long">
      <generator class="native">
        <param name="sequence">seq_ioc_table</param>
      </generator>
    </id>
    
    <property name="iocName" column="ioc_name" type="string"/>
    <property name="system" column="system" type="string"/>
    <property name="active" column="active" type="string"/>
  </class>
  
</hibernate-mapping>
