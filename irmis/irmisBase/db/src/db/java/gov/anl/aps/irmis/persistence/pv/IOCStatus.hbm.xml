<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="gov.anl.aps.irmis.persistence.pv.IOCStatus" 
         table="ioc_status" mutable="false">
    
    <id name="id" column="ioc_status_id" type="long">
      <generator class="native">
        <param name="sequence">seq_ioc_status</param>
      </generator>
    </id>

    <property name="name" column="ioc_status" type="string"/>

  </class>
  
</hibernate-mapping>
