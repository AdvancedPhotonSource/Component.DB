<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="gov.anl.aps.irmis.persistence.login.RoleName" 
         table="role_name" lazy="false" mutable="false">
    
    <id name="id" column="role_name_id" type="long">
      <generator class="native">
        <param name="sequence">seq_role_name</param>
      </generator>
    </id>

    <property name="roleName" type="string" column="role_name"/>
  </class>
  
</hibernate-mapping>
