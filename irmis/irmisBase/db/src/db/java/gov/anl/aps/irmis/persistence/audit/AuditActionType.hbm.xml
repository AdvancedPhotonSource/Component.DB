<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="gov.anl.aps.irmis.persistence.audit.AuditActionType" 
         table="audit_action_type" mutable="false">
    
    <id name="id" column="audit_action_type_id" type="long">
      <generator class="native">
        <param name="sequence">seq_audit_action_type</param>
      </generator>
    </id>

    <property name="actionName" column="action_name" type="string"/>

  </class>
  
</hibernate-mapping>
