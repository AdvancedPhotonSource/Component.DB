<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="gov.anl.aps.irmis.persistence.component.FormFactor" 
         table="form_factor" mutable="true">
    
    <id name="id" column="form_factor_id" type="long">
      <generator class="native">
        <param name="sequence">seq_form_factor</param>
      </generator>
    </id>

    <version name="version" column="version"/>

    <property name="formFactor" column="form_factor" type="string"/>
    <property name="markForDelete" column="mark_for_delete" type="boolean"/>
  </class>
  
</hibernate-mapping>
