<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="gov.anl.aps.irmis.persistence.component.PortPinType" 
         table="port_pin_type" mutable="true">
    
    <id name="id" column="port_pin_type_id" type="long">
      <generator class="native">
        <param name="sequence">seq_port_pin_type</param>
      </generator>
    </id>

    <property name="portPinType" column="port_pin_type" type="string"/>
  </class>
  
</hibernate-mapping>
