#!/bin/sh

# Run ant

if [ -z $CMS_ROOT_DIR ]; then
    cd `dirname $0` && myDir=`pwd`
    setupFile=$myDir/../../setup.sh
    if [ ! -f $setupFile ]; then
        echo "Cannot find setup file: $setupFile"
        exit 1
    fi
    source $setupFile
fi

CMS_HOST_ARCH=`uname | tr [A-Z] [a-z]`-`uname -m`
export JAVA_HOME=$CMS_SUPPORT/java/$CMS_HOST_ARCH
export ANT_HOME=$CMS_SUPPORT/ant
export PATH=$ANT_HOME/bin:$PATH
ant -Dlibs.CopyLibs.classpath=lib/org-netbeans-modules-java-j2seproject-copylibstask.jar $@


