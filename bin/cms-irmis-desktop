#!/bin/sh

if [ -z $DAQ_ROOT_DIR ]; then
    cd `dirname $0` && myDir=`pwd`
    setupFile=$myDir/../setup.sh
    if [ ! -f $setupFile ]; then
        echo "Cannot find setup file: $setupFile"
		exit 1
    fi
    source $setupFile
fi

uiJar=$CMS_ROOT_DIR/irmis/irmisBase/apps/deploy/irmisDeploy.tar
if [ ! -f $uiJar ]; then
    echo "Cannot file IRMIS jar file $uiJar."
    exit 1
fi
java -jar $uiJar


